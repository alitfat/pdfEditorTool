<?xml version='1.0' encoding='utf-8'?>
<ExcelSheetSelectedList>
  <ToolName>
    <!--ツール名称設定-->
    <widTool>
      <Name>widTool</Name>
      <QtWidget>QWidget</QtWidget>
      <WindowTitle>Excelファイルシート選択</WindowTitle>
    </widTool>
  </ToolName>
  <!--【Excelファイルシート選択】GUI-->
  <actionToolList>
    <lbFileAddr>
      <Name>lbFileAddr</Name>
      <QtWidget>QLabel</QtWidget>
      <Text>Excelファイルアドレス</Text>
      <Hidden>False</Hidden>
    </lbFileAddr>
    <txFileAddr>
      <Name>txFileAddr</Name>
      <QtWidget>QTextEdit</QtWidget>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </txFileAddr>
    <cbOneSheet>
      <Name>cbOneSheet</Name>
      <QtWidget>QCheckBox</QtWidget>
      <Text>シート毎にpdf出力(未実装)</Text>
      <Checked>False</Checked>
      <Enabled>False</Enabled>
      <Hidden>False</Hidden>
    </cbOneSheet>
    <lbExcelSheet>
      <Name>lbExcelSheet</Name>
      <QtWidget>QLabel</QtWidget>
      <Text>Excelファイルシートリスト</Text>
      <Hidden>False</Hidden>
    </lbExcelSheet>
    <lstExcelSheet>
      <Name>lstExcelSheet</Name>
      <QtWidget>QListWidget</QtWidget>
      <Hidden>False</Hidden>
    </lstExcelSheet>
    <btAddAllSheet>
      <Name>btAddAllSheet</Name>
      <QtWidget>QPushButton</QtWidget>
      <Text>&gt;&gt;\n(AddAllSheets)</Text>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </btAddAllSheet>
    <btAddOneSheet>
      <Name>btAddOneSheet</Name>
      <QtWidget>QPushButton</QtWidget>
      <Text>&gt;\n(AddSheets)</Text>
      <Enabled>False</Enabled>
      <Hidden>False</Hidden>
    </btAddOneSheet>
    <btdelOneSheet>
      <Name>btdelOneSheet</Name>
      <QtWidget>QPushButton</QtWidget>
      <Text>&lt;\n(DelSheets)</Text>
      <Enabled>False</Enabled>
      <Hidden>False</Hidden>
    </btdelOneSheet>
    <btdelAllSheet>
      <Name>btdelAllSheet</Name>
      <QtWidget>QPushButton</QtWidget>
      <Text>&lt;&lt;\n(DelAllSheets)</Text>
      <Enabled>False</Enabled>
      <Hidden>False</Hidden>
    </btdelAllSheet>
    <lbSelectedSheet>
      <Name>lbSelectedSheet</Name>
      <QtWidget>QLabel</QtWidget>
      <Text>選択されたシートリスト</Text>
      <Hidden>False</Hidden>
    </lbSelectedSheet>
    <lstSelectedSheet>
      <Name>lstSelectedSheet</Name>
      <QtWidget>QListWidget</QtWidget>
      <Hidden>False</Hidden>
    </lstSelectedSheet>
    <!--【確定】設定-->
    <btObjFileName>
      <Name>btObjFileName</Name>
      <QtWidget>QPushButton</QtWidget>
      <Text>確定</Text>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </btObjFileName>
  </actionToolList>
</ExcelSheetSelectedList>