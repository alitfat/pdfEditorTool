<?xml version='1.0' encoding='utf-8'?>
<UserAccessPermissionsSetting>
  <ToolName>
    <!--ツール名称設定-->
    <widTool>
      <Name>widTool</Name>
      <QtWidget>QWidget</QtWidget>
      <WindowTitle>Pdfファイル権限設定</WindowTitle>
    </widTool>
  </ToolName>
  <!--【pdf権限設定】GUI-->
  <actionToolList>
    <cbPrint>
      <Name>cbPrint</Name>
      <QtWidget>QCheckBox</QtWidget>
      <Text>印刷禁止</Text>
      <Checked>True</Checked>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </cbPrint>
    <cbContentCopy>
      <Name>cbContentCopy</Name>
      <QtWidget>QCheckBox</QtWidget>
      <Text>内容コピー禁止</Text>
      <Checked>True</Checked>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </cbContentCopy>
    <cbDocModify>
      <Name>cbDocModify</Name>
      <QtWidget>QCheckBox</QtWidget>
      <Text>ドキュメントの変更とアセンブリ禁止</Text>
      <Checked>True</Checked>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </cbDocModify>
    <cbCommentModify>
      <Name>cbCommentModify</Name>
      <QtWidget>QCheckBox</QtWidget>
      <Text>コメント記入禁止</Text>
      <Checked>True</Checked>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </cbCommentModify>
    <cbFillFromFields>
      <Name>cbFillFromFields</Name>
      <QtWidget>QCheckBox</QtWidget>
      <Text>ドキュメントのアセンブリとページ抽出禁止</Text>
      <Checked>True</Checked>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </cbFillFromFields>
    <!--【確定】設定-->
    <btObjFileName>
      <Name>btObjFileName</Name>
      <QtWidget>QPushButton</QtWidget>
      <Text>確定</Text>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </btObjFileName>
  </actionToolList>
</UserAccessPermissionsSetting>