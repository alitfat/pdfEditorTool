<?xml version='1.0' encoding='utf-8'?>
<pdfEditorTool>
  <ToolName>
    <!--ツール名称設定-->
    <widTool>
      <Name>widTool</Name>
      <QtWidget>QWidget</QtWidget>
      <WindowTitle>PDF情報設定</WindowTitle>
    </widTool>
  </ToolName>
  <!--【各pdf情報設定】GUI-->
  <actionToolList>
    <!--【タイトル】設定-->
    <lbTitle>
      <Name>lbTitle</Name>
      <QtWidget>QLabel</QtWidget>
      <Text>タイトル</Text>
      <Hidden>False</Hidden>
    </lbTitle>
    <txTitle>
      <Name>txTitle</Name>
      <QtWidget>QTextEdit</QtWidget>
      <Text />
      <Enabled>False</Enabled>
      <Hidden>False</Hidden>
    </txTitle>
    <cbTitle>
      <Name>cbTitle</Name>
      <QtWidget>QCheckBox</QtWidget>
      <Text>選択</Text>
      <Checked>False</Checked>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </cbTitle>
    <!--【件名】設定-->
    <lbSubject>
      <Name>lbSubject</Name>
      <QtWidget>QLabel</QtWidget>
      <Text>件名</Text>
      <Hidden>False</Hidden>
    </lbSubject>
    <txSubject>
      <Name>txSubject</Name>
      <QtWidget>QTextEdit</QtWidget>
      <Text />
      <Enabled>False</Enabled>
      <Hidden>False</Hidden>
    </txSubject>
    <cbSubject>
      <Name>cbSubject</Name>
      <QtWidget>QCheckBox</QtWidget>
      <Text>選択</Text>
      <Checked>False</Checked>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </cbSubject>
    <!--【作成者】設定-->
    <lbAuthor>
      <Name>lbAuthor</Name>
      <QtWidget>QLabel</QtWidget>
      <Text>作成者</Text>
      <Hidden>False</Hidden>
    </lbAuthor>
    <txAuthor>
      <Name>txAuthor</Name>
      <QtWidget>QTextEdit</QtWidget>
      <Text />
      <Enabled>False</Enabled>
      <Hidden>False</Hidden>
    </txAuthor>
    <cbAuthor>
      <Name>cbAuthor</Name>
      <QtWidget>QCheckBox</QtWidget>
      <Text>選択</Text>
      <Checked>False</Checked>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </cbAuthor>
    <!--【タグ】設定-->
    <lbKeywords>
      <Name>lbKeywords</Name>
      <QtWidget>QLabel</QtWidget>
      <Text>タグ</Text>
      <Hidden>False</Hidden>
    </lbKeywords>
    <txKeywords>
      <Name>txKeywords</Name>
      <QtWidget>QTextEdit</QtWidget>
      <Text />
      <Enabled>False</Enabled>
      <Hidden>False</Hidden>
    </txKeywords>
    <cbKeywords>
      <Name>cbKeywords</Name>
      <QtWidget>QCheckBox</QtWidget>
      <Text>選択</Text>
      <Checked>False</Checked>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </cbKeywords>
    <!--【Producer】設定-->
    <lbProducer>
      <Name>lbProducer</Name>
      <QtWidget>QLabel</QtWidget>
      <Text>Producer</Text>
      <Hidden>False</Hidden>
    </lbProducer>
    <txProducer>
      <Name>txProducer</Name>
      <QtWidget>QTextEdit</QtWidget>
      <Text>パソールクロステクノロジー</Text>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </txProducer>
    <cbProducer>
      <Name>cbProducer</Name>
      <QtWidget>QCheckBox</QtWidget>
      <Text>選択</Text>
      <Checked>True</Checked>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </cbProducer>
    <!--【Creator】設定-->
    <lbCreator>
      <Name>lbCreator</Name>
      <QtWidget>QLabel</QtWidget>
      <Text>Creator</Text>
      <Hidden>False</Hidden>
    </lbCreator>
    <txCreator>
      <Name>txCreator</Name>
      <QtWidget>QTextEdit</QtWidget>
      <Text>pdfEditorTool</Text>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </txCreator>
    <cbCreator>
      <Name>cbCreator</Name>
      <QtWidget>QCheckBox</QtWidget>
      <Text>選択</Text>
      <Checked>True</Checked>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </cbCreator>
    <!--【確定】設定-->
    <btObjFileName>
      <Name>btObjFileName</Name>
      <QtWidget>QPushButton</QtWidget>
      <Text>確定</Text>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </btObjFileName>
  </actionToolList>
</pdfEditorTool>